<Window x:Class="UI.EnterNameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="EnterNameWindow" SizeToContent="WidthAndHeight" WindowStartupLocation="CenterScreen"
        FontSize="14">
    <Grid>
        <StackPanel Margin="10">
            <Label Content="Название колонки" />
            <TextBox x:Name="TextBox"></TextBox>
            <Button Margin="0,5,0,0" Content="ok" Click="Button_Click"></Button>
        </StackPanel>
    </Grid>
</Window>
